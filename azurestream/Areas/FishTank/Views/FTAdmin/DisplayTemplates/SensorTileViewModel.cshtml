@model apa.BOL.FishTank.ChartViewModels.SensorTileViewModel

<!--FISH TANK SENSOR TILE -->
<div class="card mb-2" style="width: 14rem;">
    <div class="card-header bg-primary @Model.ColorCssClass"><p class="p-0 m-0"><i class="fa @Model.IconCssClass fa pr-1"></i> @Model.Title</p></div>
    <div class="card-body text-center">
        <p class="card-text text-center h5">@Model.Value</p>
        <a class="card-link" href="@Model.Url">View Details</a>
        <a class="card-link" asp-controller="fthistory" asp-action="@Model.Url"><i class="fa fa-chevron-right"></i></a>
    </div>
</div>
<!--FISH TANK SENSOR TILE -->
