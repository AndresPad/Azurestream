@{
    ViewData["Title"] = "Build Amazing Cloud and IoT Ecosystems | About";
    Layout = "_AppLayoutFlex";
}

<!-- Start About -->
<section class="about section-full relative" id="about">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-6 col-md-8 col-10">
                <div class="section-title text-center pb-50">
                    <h3>About Azurestream</h3>
                    <img src="~/images/icon/title-bottom-4.png" alt="" />

                    <h2>@ViewData["Title"]</h2>
